{
  "name": "cconsent-vue",
  "version": "2.0.0",
  "description": "Vue 3 adapter for cconsent cookie consent",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "typecheck": "tsc --noEmit"
  },
  "keywords": ["cookie", "consent", "gdpr", "vue", "vue3", "composables"],
  "author": "pxdsgnco",
  "license": "MIT",
  "peerDependencies": {
    "vue": ">=3.0.0",
    "cconsent": "^2.0.0"
  },
  "devDependencies": {
    "vue": "^3.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "cconsent": "^2.0.0"
  }
}
