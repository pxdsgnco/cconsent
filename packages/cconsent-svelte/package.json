{
  "name": "cconsent-svelte",
  "version": "2.0.0",
  "description": "Svelte adapter for cconsent cookie consent",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "svelte": "dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.mjs"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "typecheck": "tsc --noEmit"
  },
  "keywords": ["cookie", "consent", "gdpr", "svelte", "sveltekit", "stores"],
  "author": "pxdsgnco",
  "license": "MIT",
  "peerDependencies": {
    "svelte": ">=3.0.0 || >=4.0.0 || >=5.0.0",
    "cconsent": "^2.0.0"
  },
  "devDependencies": {
    "svelte": "^4.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "cconsent": "^2.0.0"
  }
}
