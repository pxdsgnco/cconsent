{
  "name": "cconsent-react",
  "version": "2.0.0",
  "description": "React adapter for cconsent cookie consent",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup src/index.tsx --format cjs,esm --dts",
    "dev": "tsup src/index.tsx --format cjs,esm --dts --watch",
    "typecheck": "tsc --noEmit"
  },
  "keywords": ["cookie", "consent", "gdpr", "react", "hooks"],
  "author": "pxdsgnco",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=17.0.0",
    "cconsent": "^2.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "react": "^18.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "cconsent": "^2.0.0"
  }
}
